local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("NothingYoko", "Midnight")
local Tab = Window:NewTab("Bloxburg")
local Section = Tab:NewSection("YokoHUB")
Section:NewButton("FishFarm", "ButtonInfo", function()
getgenv().ScriptJob = "FisherMan" -- Job Autofarm
getgenv().ScriptKey = "3CKLJoyl6E9ahPL0l3LLz0QscNmhhp"
loadstring(game:HttpGet("https://raw.githubusercontent.com/SurgeDid/Blockburg/main/loader.lua"))();
    print("Clicked")
end)
Section:NewButton("Hairdresser", "ButtonInfo", function()
getgenv().HairDresserStation = "3" -- 1,2,3,4 are your options
getgenv().ScriptJob = "HairDresser" -- Job Autofarm
getgenv().ScriptKey = "3CKLJoyl6E9ahPL0l3LLz0QscNmhhp"
loadstring(game:HttpGet("https://raw.githubusercontent.com/SurgeDid/Blockburg/main/loader.lua"))();
    print("Clicked")
end)
Section:NewButton("PizzaDelivery", "ButtonInfo", function()
getgenv().ScriptJob = "PizzaDelivery" -- Job Autofarm
